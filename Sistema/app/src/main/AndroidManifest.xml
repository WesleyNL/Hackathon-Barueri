<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.com.insure">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/icone"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".activity.BemVindoActivity"/>
        <activity android:name=".activity.LoginActivity" />
        <activity android:name=".activity.CadastroActivity" />
        <activity android:name=".activity.SegurosActivity" />
        <activity android:name=".activity.CadastrarPropostaActivity" />
        <activity android:name=".activity.AppSplashScreenActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>